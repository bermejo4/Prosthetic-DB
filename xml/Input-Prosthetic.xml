<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!DOCTYPE prosthetic [
	<!ELEMENT prosthetic (material, price, dimensions, failures, available,patient, biomeds?)>
	<!ATTLIST prosthetic type CDATA #REQUIRED>
	<!ELEMENT material (#PCDATA)>
	<!ELEMENT price (#PCDATA)>
	<!ELEMENT dimensions (#PCDATA)>
	<!ELEMENT failures (#PCDATA)>
	<!ELEMENT available (#PCDATA)>
	<!ELEMENT patient (name, lastname)>
	<!ATTLIST patient id CDATA #REQUIRED>
	<!ELEMENT name (#PCDATA)>
	<!ELEMENT lastname (#PCDATA)>
	<!ELEMENT biomeds (biomed+)>
	<!ELEMENT biomed (name, lastname, telephone)>
	<!ATTLIST biomed id CDATA #REQUIRED>
	<!ELEMENT name (#PCDATA)>
	<!ELEMENT lastname (#PCDATA)>
	<!ELEMENT telephone (#PCDATA)>
]>

<prosthetic type = "hand">
	<material>plastic</material>
	<price>1000</price>
	<dimensions>30x30x30</dimensions>
	<failures>none</failures>
    <available>false</available>
    <patient id="4">
    	<name>Rocio</name>
       <lastname>Ruiz</lastname>
    </patient>
    <biomeds>
    	<biomed id="2">
    		<name>Marina</name>
    		<lastname>Miguelez</lastname>
    		<telephone>Marina</telephone>
    	</biomed>
   </biomeds>
</prosthetic>